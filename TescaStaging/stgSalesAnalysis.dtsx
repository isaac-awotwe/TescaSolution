                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">if (select count(*) from TescaDB.EDW.fact_SalesAnalysis) &lt;= 0
	select s.TransactionID,s.TransactionNo,convert(date,s.TransDate) as TransDate,DATEPART(hour,s.TransDate) as TransHour,
	CONVERT(date,s.OrderDate) as OrderDate, DATEPART(hour,s.OrderDate) as OrderHour,
	CONVERT(date,s.DeliveryDate) as DeliveryDate,
	s.ChannelID,s.CustomerID,s.EmployeeID,s.ProductID,s.StoreID,s.PromotionID,
	s.Quantity,s.TaxAmount,s.LineAmount,s.LineDiscountAmount, getdate() as LoadDate from TescaOLTP.dbo.SalesTransaction s
	where convert(date,s.TransDate) &lt;= DATEADD(day,-1, convert(date,getdate()))
else
	select s.TransactionID,s.TransactionNo,convert(date,s.TransDate) as TransDate,DATEPART(hour,s.TransDate) as TransHour,
	CONVERT(date,s.OrderDate) as OrderDate, DATEPART(hour,s.OrderDate) as OrderHour,
	CONVERT(date,s.DeliveryDate) as DeliveryDate,
	s.ChannelID,s.CustomerID,s.EmployeeID,s.ProductID,s.StoreID,s.PromotionID,
	s.Quantity,s.TaxAmount,s.LineAmount,s.LineDiscountAmount, getdate() as LoadDate from TescaOLTP.dbo.SalesTransaction s
	where convert(date,s.TransDate) = DATEADD(day,-1, convert(date,getdate()))</property>
